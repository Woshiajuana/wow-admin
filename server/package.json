{
    "name": "wow-admin-service",
    "version": "1.0.0",
    "description": "",
    "private": true,
    "egg": {
        "declarations": true
    },
    "dependencies": {
        "apidoc": "^0.17.7",
        "egg": "^2.15.1",
        "egg-wow-admin-framework": "0.0.7",
        "egg-redis": "^2.4.0",
        "egg-cors": "^2.2.0",
        "egg-mongoose": "^3.2.0",
        "egg-wow-jwt": "0.0.2",
        "egg-wow-response": "0.0.1",
        "egg-wow-validate": "0.0.1",
        "egg-scripts": "^2.11.0",
        "ms": "^2.1.2"
    },
    "devDependencies": {
        "autod": "^3.0.1",
        "autod-egg": "^1.1.0",
        "egg-bin": "^4.11.0",
        "egg-ci": "^1.11.0"
    },
    "engines": {
        "node": ">=10.0.0"
    },
    "scripts": {
        "start": "egg-scripts start --daemon --title=template-admin-service --framework=egg-wow-admin-framework",
        "stop": "egg-scripts stop --title=template-admin-service",
        "dev": "npm run apidoc && egg-bin dev --framework=egg-wow-admin-framework",
        "debug": "egg-bin debug",
        "test": "npm run lint -- --fix && npm run test-local",
        "test-local": "egg-bin test",
        "cov": "egg-bin cov",
        "lint": "eslint .",
        "ci": "npm run lint && npm run cov",
        "apidoc": "apidoc -i app/controller -o app/public/apidoc/",
        "autod": "autod"
    },
    "ci": {
        "version": "10"
    },
    "repository": {
        "type": "git",
